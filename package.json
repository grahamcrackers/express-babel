{
  "name": "express-babel",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec npm run babel-node -- ./src/index.js",
    "build": "babel app -d dist",
    "clean": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "mongoose": "^4.12.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint",
    "ava"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  }
}
